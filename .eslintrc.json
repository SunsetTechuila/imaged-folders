{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2022": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "airbnb-typescript/base",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "project": ["tsconfig.json"]
  },
  "plugins": ["@typescript-eslint"]
}
